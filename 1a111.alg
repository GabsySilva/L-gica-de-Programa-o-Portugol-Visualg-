algoritmo "semnome"
// Função :
// Autor :
// Data : 04/09/2023
// Seção de Declarações 
var
   par, num: inteiro

inicio

   escreval("-----Números pares--------")
   escreval()

   escreval("Digite um número")
   leia(num)

   par <- 0
   enquanto (par <= num) faca
      escreval(par)
      par <- par + 2
   fimenquanto
   
fimalgoritmo
